{
  "expo": {
    "name": "rezzy-app",
    "slug": "rezzy-app",
    "scheme": "rezzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.rezzy.app",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.rezzy.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-apple-authentication"
    ],
    "extra": {
      "googleClientIdAndroid": "661428302727-n3jdtangvmk094kcn2340tqeoadl9jpl.apps.googleusercontent.com",
      "googleClientIdIos": "YOUR-IOS-CLIENT-ID.apps.googleusercontent.com",
      "googleClientIdWeb": "661428302727-rh362fibpvi2ug1b6phh6ogc9mno45bq.apps.googleusercontent.com",
      "eas": {
        "projectId": "49189fbc-e9bb-4507-a5c7-713e5c73b3f6"
      }
    }
  }
}
